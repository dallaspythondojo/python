<html>
<head>
  <title></title>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script type="text/javascript">

  $(docuemnt).ready(function(){
    function rBinarySearch(list, val){
        var end = list.length() - 1;
        var front = 1;
        var mid = Math.floor(list.length()/2)

        if (val == list[mid] || val == list[front] || val == list[end]){
            return True;
        }
        else if (val > list[mid]){
            return rBinarySearch(list.slice(mid, end+1), val);
        }
        else if (val < list[mid]){
            return rBinarySearch(list.slice(front,mid), val);
        }
        else {
            return false;
        }
        });
    }
  });


  $(function(){
    // or here, pick one.
  });
  </script>
</head>
<body>

</body>
</html>
